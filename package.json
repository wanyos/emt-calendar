{
  "name": "emt-calendar",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "connect-db": "node dummy-server/db/mysql.js",
    "dummy": "node --watch dummy-server/index.js",
    "dev:open": "vite --open --mode development",
    "serve:dummy": "npm run dummy & npm run dev:open",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:unit": "cypress run --component",
    "test:unit:dev": "cypress open --component",
    "lint": "eslint \"src/**/*.{js,vue}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write src/",
    "stop:servers": "kill $(lsof -t -i:8022) & kill $(lsof -t -i:5173)"
  },
  "dependencies": {
    "@azure/core-http": "^3.0.3",
    "@azure/msal-browser": "^3.22.0",
    "@azure/storage-blob": "^12.24.0",
    "@heroicons/vue": "^2.1.5",
    "dotenv": "^16.4.5",
    "firebase": "^10.13.1",
    "firebase-admin": "^12.4.0",
    "npm-check-updates": "^17.1.1",
    "pinia": "^2.2.2",
    "tiny-emitter": "^2.1.0",
    "vue": "^3.4.38",
    "vue-cookies": "^1.8.4",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@vitejs/plugin-vue": "^5.1.3",
    "autoprefixer": "^10.4.20",
    "cors": "^2.8.5",
    "cypress": "^13.14.1",
    "eslint": "^9.9.1",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-vue": "^9.27.0",
    "express": "^4.19.2",
    "mysql2": "^3.11.0",
    "postcss": "^8.4.42",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.5",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.2"
  }
}
