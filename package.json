{
  "name": "emt-calendar",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "connect-db": "node dummy-server/db/mysql.js",
    "dummy": "node dummy-server/index.js",
    "dev:open": "vite --open --mode development",
    "serve:dummy": "node --watch dummy-server/index.js & npm run dev:open",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:unit": "cypress run --component",
    "test:unit:dev": "cypress open --component",
    "lint": "eslint \"src/**/*.{js,vue}\" \"dummy-server/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write src/",
    "stop:servers": "kill $(lsof -t -i:8022) & kill $(lsof -t -i:5173)",
    "test": "vitest"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "firebase": "^10.14.0",
    "firebase-admin": "^12.6.0",
    "google-auth-library": "^9.14.1",
    "jsonwebtoken": "^9.0.2",
    "npm-check-updates": "^17.1.3",
    "oh-vue-icons": "^1.0.0-rc3",
    "pinia": "^2.2.4",
    "tiny-emitter": "^2.1.0",
    "vue": "^3.5.11",
    "vue-cookies": "^1.8.4",
    "vue-router": "^4.4.5",
    "vue3-toastify": "^0.2.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "cors": "^2.8.5",
    "cypress": "^13.15.0",
    "eslint": "^9.12.0",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^9.28.0",
    "express": "^4.21.0",
    "mysql2": "^3.11.3",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.8",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.13",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  }
}
